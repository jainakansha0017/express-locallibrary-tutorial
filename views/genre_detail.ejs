
    <h1>Genre : <%= genre.name %></h1>
    <div style='margin-left:20px;margin-top:20px'>
      <h4> Books</h4>
      <dl>
      <% genre_books.forEach(function(books)
      { %>
        <dt><a href='<%= books.url %>'> <%= books.title %></a></dt>
        <dd><%= books.summary%></dd>
      <% }) %>
      </dl>
      <% if (genre_books.length==0) { %>
        <p> The genre has no books.</p>
      <% } %>
      </div>
